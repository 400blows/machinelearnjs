# {{ name }}

{{{ comment.shortText }}}
{{{ comment.text }}}

{{#ifEquals this this.kindString "Function"}}

    {{#isSignatureValid this}}
        {{#each this.signatures}}
            {{newLine}}▸ **{{this.name}}**{{methodBracket this.parameters}}
            {{newLine}}{{{this.comment.shortText}}}
            {{newLine}}{{{this.comment.text}}}
            {{#filterTagExample this.comment.tags}}
                {{newLine}}### Usage
                {{#each this}}
                    {{newLine}}```javascript{{{this.text}}}```
                {{/each}}
            {{/filterTagExample}}
        {{/each}}
        {{newLine}}Defined in {{getSourceLink this.sources}}

        {{newLine}}**Parameters:**
        {{#each this.signatures}}
            {{newLine}}{{constructParamTable this.parameters}}
            {{newLine}}**Returns:** {{renderMethodReturnType this.type}}
        {{/each}}
    {{/isSignatureValid}}

{{else}}

    {{#filterTagExample this.comment.tags}}
        {{newLine}}### Usage
        {{#each this}}
            {{newLine}}```javascript{{{this.text}}}```
        {{/each}}
    {{/filterTagExample}}

    {{#filterConstructor children}}
        {{newLine}}### Constructors
        {{#each this}}
            {{newLine}}* [constructor](#constructor)
        {{/each}}
    {{/filterConstructor}}

    {{#filterProperty children}}
        {{newLine}}### Properties
        {{#each this}}
            {{newLine}}* [{{this.name}}](#▸-{{cleanHyperLink this.name}})
        {{/each}}
    {{/filterProperty}}

    {{#filterMethod children}}
        {{newLine}}### Methods
        {{#each this}}
            {{newLine}}* [{{this.name}}](#λ-{{cleanHyperLink this.name}})
        {{/each}}
    {{/filterMethod}}



    {{#filterConstructor children}}

        {{newLine}}# Constructors
        {{newLine}}---
        {{#each this}}
            {{newLine}}### {{this.name}}
            {{newLine}}{{ this.comment.shortText }}
            {{newLine}}{{ this.comment.text }}

            {{#isSignatureValid this}}
                {{#each this.signatures}}
                    {{newLine}}⊕ **{{this.name}}**{{methodBracket this.parameters}}
                {{/each}}
                {{newLine}}Defined in {{getSourceLink this.sources}}
                {{newLine}}{{this.comment.shortText}}

                    {{newLine}}**Parameters:**
                    {{#each this.signatures}}
                        {{newLine}}{{constructParamTable this.parameters}}
                        {{newLine}}**Returns:** {{ this.type.name }}
                    {{/each}}
            {{else}}
                {{newLine}}No parameters required to invoke!
            {{/isSignatureValid}}

        {{/each}}
    {{/filterConstructor}}



    {{#filterProperty children}}

        {{newLine}}# Properties
        {{newLine}}---
        {{#each this}}
            {{newLine}}### ▸ {{this.name}}
            {{newLine}}{{ this.comment.shortText }}
            {{newLine}}{{ this.comment.text }}
            {{newLine}}Defined in {{getSourceLink this.sources}}
        {{/each}}
    {{/filterProperty}}

    {{#filterMethod children}}

        {{newLine}}# Methods
        {{newLine}}---
        {{#each this}}
            {{#isSignatureValid this}}
                {{#each this.signatures}}
                    {{newLine}}### λ {{this.name}}
                    {{newLine}}{{ this.comment.shortText }}
                    {{newLine}}{{ this.comment.text }}
                {{/each}}
                {{newLine}}Defined in {{getSourceLink this.sources}}
                {{newLine}}{{this.comment.shortText}}

                {{newLine}}**Parameters:**
                {{#each this.signatures}}
                    {{newLine}}{{constructParamTable this.parameters}}
                    {{newLine}}**Returns:** {{renderMethodReturnType this.type}}
                {{/each}}
            {{else}}
                {{newLine}} Empty parameters required to invoke!
            {{/isSignatureValid}}
        {{/each}}
    {{/filterMethod}}

{{/ifEquals}}