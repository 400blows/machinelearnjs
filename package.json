{
  "name": "kalimdor",
  "version": "1.0.0",
  "description": "Machine Learning library for the web and Node",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/JasonShin/kalimdorjs",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "info": "npm-scripts-info",
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "npx prettier \"src/**/*.ts\" \"test/**/*.ts\" \"docs/**/*.ts\" --config ./.prettierrc --write",
    "test": "npx run-s build test:*",
    "test:lint": "npx tslint --force -t verbose 'test/**/*.ts' 'src/**/*.ts'",
    "test:unit": "npx jest",
    "test:nsp": "nsp check",
    "watch": "./scripts/tmux-start.sh",
    "watch:build": "yarn build:main -- -w",
    "watch:unit": "yarn test:unit --watch",
    "watch:lint": "watch \" npx tslint --force -t verbose 'test/**/*.ts' 'src/**/*.ts'\" src",
    "cov": "run-s build test:unit cov:html && opn coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:send": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "rm -rf docs/md_out && yarn doc:json && tsc ./docs/processor/index.ts && node ./docs/processor",
    "doc:md": "rm -rf docs/md_out && tsc ./docs/processor/index.ts && node ./docs/processor",
    "doc:json": "npx typedoc --json docs/docs.json --tsconfig tdconfig.json --excludePrivate",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version": "standard-version",
    "reset": "git clean -dfx && git reset --hard && npm i",
    "clean": "trash build",
    "all": "run-s reset test cov:check doc:html",
    "prepare-release": "run-s all version doc:publish"
  },
  "//": {
    "doc:md": "rm -rf docs/md_base && npx typedoc --out docs/md_base --theme markdown --tsconfig tdconfig.json --excludePrivate",
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Warning('It is recommended to install Kalimdorjs with Yarn: https://yarnpkg.com/')\""
  },
  "scripts-info": {
    "info": "Display information about the package scripts",
    "build": "Clean and rebuild the project",
    "fix": "Try to automatically fix any linting problems",
    "test": "Lint and unit test the project",
    "watch": "Watch and rebuild the project on save, then rerun relevant tests",
    "cov": "Rebuild, run tests, then create and open the coverage report",
    "doc": "Generate HTML API documentation and open it in a browser",
    "doc:json": "Generate API documentation in typedoc JSON format",
    "version": "Bump package.json version, update CHANGELOG.md, tag release",
    "reset": "Delete all untracked files and reset the repo to the last commit",
    "prepare-release": "One-step: clean, build, test, publish docs, and prep a release"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.108",
    "@types/mathjs": "^3.20.0",
    "@types/node": "8.9.4",
    "@types/random-js": "^1.0.30",
    "jest": "^22.4.3",
    "libsvm-js": "^0.2.0",
    "lodash": "^4.17.10",
    "mathjs": "^4.2.1",
    "ml-confusion-matrix": "^0.3.0",
    "ml-distance-euclidean": "^1.0.0",
    "ml-regression-simple-linear": "^1.0.2",
    "ml-sparse-matrix": "^1.0.0",
    "natural": "^0.5.6",
    "nodemon": "^1.17.4",
    "random-js": "^1.0.8",
    "sha.js": "^2.4.10",
    "stopword": "^0.1.10",
    "ts-jest": "^22.4.5",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.3",
    "codecov": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "gh-pages": "^1.0.0",
    "handlebars": "^4.0.11",
    "npm-run-all": "^4.1.2",
    "npm-scripts-info": "^0.3.6",
    "nsp": "^3.2.1",
    "nyc": "^11.5.0",
    "opn-cli": "^3.1.0",
    "prettier": "^1.10.2",
    "standard-version": "^4.0.0",
    "trash-cli": "^1.4.0",
    "tslint": "^5.4.3",
    "tslint-config-prettier": "^1.8.0",
    "tslint-immutable": "^4.5.1",
    "typedoc": "^0.11.1",
    "typedoc-plugin-markdown": "^1.1.13",
    "typescript": "^2.4.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.(ts|tsx|js)$",
    "verbose": true
  },
  "ava": {
    "failFast": true,
    "files": [
      "build/main/**/*.spec.js"
    ],
    "sources": [
      "build/main/**/*.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": [
      "@types/node"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
